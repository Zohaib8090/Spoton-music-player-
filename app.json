{
  "expo": {
    "name": "Spoton Music Player",
    "slug": "spoton-music-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/new_icon.png",
    "scheme": "spotonmusicplayer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zohaib144.spotonmusicplayer",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/new_icon.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.zohaib144.spotonmusicplayer",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "./plugins/withAndroidDuplicateClassesFix.js",
      [
        "expo-build-properties",
        {
          "android": {
            "useAndroidX": true,
            "enableJetifier": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "767eb3ca-50b8-48fd-ae91-2a028f3915de"
      }
    }
  }
}
